graph {
  ratio = 1;
  node [fontsize=25];
  margin = 0.01;
  0 -- 1;
  0 -- 2;
  1 -- 3;
  1 -- 4;
  4 -- 7;
  4 -- 8;
  2 -- 5;
  2 -- 6;
  0 [label="-",shape=box];
  1 [label="+",shape=box];
  2 [label="+",shape=box];
  3 [label="a"];
  4 [label="-",shape=box];
  5 [label="d"];
  6 [label="e"];
  7 [label="b"];
  8 [label="c"];
}
